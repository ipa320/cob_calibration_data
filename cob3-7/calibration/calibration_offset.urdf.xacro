<?xml version="1.0" ?><robot>
	<!-- ***************** -->
	<!-- base calibration -->
	<!-- ***************** -->
	<!-- laser front mount positions | relative to base_link -->
	<property name="offset_laser_front_x" value="0.0"/>
	<property name="offset_laser_front_y" value="0.0"/>
	<property name="offset_laser_front_z" value="0.0"/>
	<property name="offset_laser_front_roll" value="0"/>
	<property name="offset_laser_front_pitch" value="0"/>
	<property name="offset_laser_front_yaw" value="0"/>

	<!-- laser rear mount positions | relative to base_link -->
	<property name="offset_laser_rear_x" value="0.0"/>
	<property name="offset_laser_rear_y" value="0.0"/>
	<property name="offset_laser_rear_z" value="0.0"/>
	<property name="offset_laser_rear_roll" value="0"/>
	<property name="offset_laser_rear_pitch" value="0"/>
	<property name="offset_laser_rear_yaw" value="0"/>

	<!-- laser top mount positions | relative to base_link -->
	<property name="offset_laser_top_x" value="0.0"/>
	<property name="offset_laser_top_y" value="0.0"/>
	<property name="offset_laser_top_z" value="0.0"/>
	<property name="offset_laser_top_roll" value="0"/>
	<property name="offset_laser_top_pitch" value="0"/>
	<property name="offset_laser_top_yaw" value="0"/>

	<!-- *********************** -->
	<!-- upper robot calibration -->
	<!-- *********************** -->
	<!-- tray mount positions | relative to base_link -->
	<property name="offset_tray_x" value="0.0"/>
	<property name="offset_tray_y" value="0.0"/>
	<property name="offset_tray_z" value="0.0"/>
	<property name="offset_tray_roll" value="0.0"/>
	<property name="offset_tray_pitch" value="0.0"/>
	<property name="offset_tray_yaw" value="0.0"/>

	<!-- torso mount positions | relative to base_link -->
	<property name="offset_torso_x" value="0"/>
	<property name="offset_torso_y" value="0"/>
	<property name="offset_torso_z" value="0"/>
	<property name="offset_torso_roll" value="0"/>
	<property name="offset_torso_pitch" value="0"/>
	<property name="offset_torso_yaw" value="0"/>

	<!-- head mount positions | relative to head_cover_link -->
	<property name="offset_head_x" value="0"/>
	<property name="offset_head_y" value="0"/>
	<property name="offset_head_z" value="0"/>
	<property name="offset_head_roll" value="0"/>
	<property name="offset_head_pitch" value="0"/>
	<property name="offset_head_yaw" value="0"/>

	<!-- ur_conector mount positions | relative to base_link --> 
	<property name="offset_ur_conector_x" value="0"/>
	<property name="offset_ur_conector_y" value="0"/>
	<property name="offset_ur_conector_z" value="0"/>
	<property name="offset_ur_conector_roll" value="0"/>
	<property name="offset_ur_conector_pitch" value="0"/>
	<property name="offset_ur_conector_yaw" value="0"/>

	<!-- arm mount positions | relative to ur_conector_link --> 
	<property name="offset_arm_x" value="0"/>
	<property name="offset_arm_y" value="0"/>
	<property name="offset_arm_z" value="0"/>
	<property name="offset_arm_roll" value="0"/>
	<property name="offset_arm_pitch" value="0"/>
	<property name="offset_arm_yaw" value="0"/>

	<!-- *********************************** -->
	<!-- kinematic chain reference positions -->
	<!-- *********************************** -->
	<!-- torso chain calibration_offest! | relative to reference position (see below)-->

	<!-- torso reference positions | used as calibration_rising for torso chain -->
	<!-- ! set these values to make torso straight with (0, 0, 0) joint angles ! -->
	<property name="offset_torso_lower_neck_tilt_ref" value="0.0"/>
	<property name="offset_torso_pan_ref" value="0.0"/>
	<property name="offset_torso_upper_neck_tilt_ref" value="0.0"/>

	<!-- tray reference position | used as calibration_rising for tray chain -->
	<property name="offset_tray_ref" value="0.0"/>

	<!-- head camera axis reference position | used as calibration_rising for head axis chain -->
	<property name="offset_head_axis_ref" value="0"/>

	<!-- arm reference positions | used as calibration_rising for arm chain-->
	<!-- not needed for lbr, use nonius scala -->

	<!-- ***************************** -->
	<!-- camera calibration properties -->
	<!-- ***************************** -->
	<!-- left | handeye calibration | relative to head_axis_link -->
	<property name="offset_cam_l_x" value="0"/>
	<property name="offset_cam_l_y" value="0"/>
	<property name="offset_cam_l_z" value="0"/>
	<property name="offset_cam_l_roll" value="0"/>
	<property name="offset_cam_l_pitch" value="0"/>
	<property name="offset_cam_l_yaw" value="0"/>

	<!-- right | stereo baseline calibration | relative to left camera -->
	<property name="offset_cam_r_x" value="0.0"/>
	<property name="offset_cam_r_y" value="0.0"/>
	<property name="offset_cam_r_z" value="0.0"/>
	<property name="offset_cam_r_roll" value="0.0"/>
	<property name="offset_cam_r_pitch" value="0.0"/>
	<property name="offset_cam_r_yaw" value="0.0"/>

	<!-- cam3d | handeye calibration | relative to head_axis_link -->
	<property name="offset_cam3d_x" value="0"/>
	<property name="offset_cam3d_y" value="0"/>
	<property name="offset_cam3d_z" value="0"/>
	<property name="offset_cam3d_roll" value="0"/>
	<property name="offset_cam3d_pitch" value="0"/>
	<property name="offset_cam3d_yaw" value="0"/>
</robot>
